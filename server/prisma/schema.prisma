generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Charcoords {
  id   Int    @id @default(autoincrement())
  name String @unique
  minX Int
  maxX Int
  minY Int
  maxY Int
}

model Highscores {
  id Int @id @default(autoincrement())
  name String
  score Int
}
